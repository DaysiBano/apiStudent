extends layoutTable

block content
		section(id="main-content")
			section(class="wrapper")
				div(class="row")
					div(class="col-lg-12")
						section(class="panel")
							header(class="panel-heading") Tabla

							table(class="table table-striped table-advance table-hover")
								tbody
									tr
										th Nombre Completo 
											i(class="icon_profile") 
										th Año de graduación 
											i(class="icon_calendar") 
										th Email 
											i(class="icon_mail_alt") 
										th Ciudad 
											i(class="icon_pin_alt") 
										th Celular 
											i(class="icon_mobile") 
										th Acciones 
											i(class="icon_cogs") 
									if (graduates)
									each gdt, index in graduates
										tr
											td= gdt.Names + ' ' + gdt.LastNames
											td= gdt.DateFinshCarrear
											td= gdt.Email
											td= gdt.Country
											td= gdt.CellPhone
											td
												div(class="btn-group")
													a(class="btn btn-primary" href="#")
														i(class="icon_plus_alt2")
													a(class="btn btn-success" href="#")
														i(class="icon_check_alt2")
													a(class="btn btn-danger" href="#")
														i(class="icon_close_alt2")